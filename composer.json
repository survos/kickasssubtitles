{
    "name": "kickasssubtitles/kickasssubtitles",
    "description": "Open-source movie and TV subtitles",
    "type": "project",
    "homepage": "https://kickasssubtitles.com",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "grzesw",
            "email": "contact@kickasssubtitles.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-simplexml": "*",
        "ext-xmlrpc": "*",
        "ext-zend-opcache": "*",
        "ext-zlib": "*",
        "fideloper/proxy": "*",
        "gemorroj/archive7z": "*",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "^2.5.1",
        "jaybizzle/laravel-crawler-detect": "^v1.2.0",
        "jenssegers/model": "^v1.4.0",
        "laravel/framework": "^v8.73.0",
        "lavary/laravel-menu": "^v1.8.3",
        "mantas-done/subtitles": "^0.3.9",
        "mariuzzo/laravel-js-localization": "^v1.8.0",
        "mcamara/laravel-localization": "^1.6",
        "myclabs/php-enum": "^1.7.2",
        "patrickschur/language-detection": "^v5.1.0",
        "php-tmdb/api": "dev-tac",
        "ramsey/uuid": ">= 4.2",
        "sentry/sentry-laravel": "dev-master",
        "spatie/laravel-medialibrary": "^9.9.0",
        "thecodingmachine/safe": "^1.3",
        "vinkla/hashids": "^9.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.6.3",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "KickAssSubtitles\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "test": [
            "phpunit"
        ],
        "tests": [
            "@test"
        ],
        "t": [
            "@test"
        ],
        "static-analysis": [
            "phpstan analyse app src tests --level=max --memory-limit=256M"
        ],
        "sa": [
            "@static-analysis"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    },
    "repositories": {
        "tmdb_api": {
            "type": "vcs",
            "url": "git@github.com:tacman/api.git"
        },
        "list_generator": {
            "type": "vcs",
            "url": "git@github.com:tacman/list-generator.git"
        }
    }
}
