{
    "name": "kickasssubtitles/kickasssubtitles",
    "description": "Open-source movie and TV subtitles",
    "type": "project",
    "homepage": "https://kickasssubtitles.com",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "grzesw",
            "email": "contact@kickasssubtitles.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-simplexml": "*",
        "ext-xmlrpc": "*",
        "ext-zend-opcache": "*",
        "ext-zlib": "*",
        "fideloper/proxy": "*",
        "gemorroj/archive7z": "*",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "2.5.1",
        "jaybizzle/laravel-crawler-detect": "*",
        "jenssegers/model": "*",
        "laravel/framework": ">= 5.0",
        "lavary/laravel-menu": "*",
        "mantas-done/subtitles": "0.3.9",
        "mariuzzo/laravel-js-localization": "*",
        "mcamara/laravel-localization": "*",
        "myclabs/php-enum": "1.7.2",
        "patrickschur/language-detection": "*",
        "php-tmdb/api": "2.1.17",
        "ramsey/uuid": "^4.2.1",
        "sentry/sentry-laravel": "*",
        "spatie/laravel-medialibrary": "*",
        "thecodingmachine/safe": "^1.3",
        "vinkla/hashids": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "umpirsky/list-generator": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "KickAssSubtitles\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "test": [
            "phpunit"
        ],
        "tests": [
            "@test"
        ],
        "t": [
            "@test"
        ],
        "static-analysis": [
            "phpstan analyse app src tests --level=max --memory-limit=256M"
        ],
        "sa": [
            "@static-analysis"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    }
}
